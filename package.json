{
  "name": "ar-viewer",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "shared",
    "web",
    "mobile"
  ],
  "scripts": {
    "web": "npm run dev --workspace=web",
    "mobile": "npm run start --workspace=mobile",
    "dev": "concurrently \"npm run web\"",
    "install:all": "npm install"
  },
  "overrides": {
    "minimatch": "^10.2.1",
    "glob": "^11.0.0"
  },
  "devDependencies": {
    "concurrently": "^9.1.0"
  }
}